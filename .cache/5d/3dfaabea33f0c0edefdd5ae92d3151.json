n();\n        const newBook = {\n            author: author,\n            title: title,\n            publicationDate: publicationDate,\n            genre: genre,\n            audience: audience,\n        };\n\n        const newBooks = [...books, newBook];\n        setBooks(newBooks);\n    };\n\n    return (\n        <div>\n            <form>\n                <InputComponent\n                    label=\"Tytuł\"\n                    state={title}\n                    setState={setTitle}\n                />\n                <br />\n                <InputComponent\n                    label=\"Autor\"\n                    state={author}\n                    setState={setAuthor}\n                />\n                <br />\n                <InputComponent\n                    label=\"Data Publikacji\"\n                    state={publicationDate}\n                    setState={setPublicationDate}\n                    type=\"number\"\n                />\n                <br />\n                <GenreDropdown />\n                <br />\n                <AudienceDropdown />\n                <button onClick={(event) => addBook(event)}>Dodaj</button>\n            </form>\n        </div>\n    );\n};\n"},"lineCount":null}},"error":null,"hash":"786923b36a9e594820ed60248dedd88d","cacheData":{"env":{}}}