st [author, setAuthor] = useState(\"\");\n    const [title, setTitle] = useState(\"\");\n    const [publicationDate, setPublicationDate] = useState(\"\");\n\n    const genres = [\"Fantastyka\", \"Horror\", \"Kryminał\"];\n    const audiences = [\"Dzieci\", \"Młodież\", \"Dorośli\"];\n\n    const [genre, GenreDropdown] = useDropdown(\"\", \"Gatunek\", genres);\n    const [audience, AudienceDropdown] = useDropdown(\"\", \"Dla\", audiences);\n\n    const addBook = (event) => {\n        event.preventDefault();\n        event.stopPropagation();\n        const newBook = {\n            author: author,\n            title: title,\n            publicationDate: publicationDate,\n            genre: genre,\n            audience: audience,\n        };\n\n        const newBooks = [...books, newBook];\n        setBooks(newBooks);\n    };\n\n    return (\n        <div>\n            <form>\n                <label htmlFor=\"title\">\n                    Tytuł\n                    <input\n                        id=\"title\"\n                        value={title}\n                        placeholder=\"Title\"\n                        onChange={(event) => {\n                            setTitle(event.target.value);\n                        }}\n                    />\n                </label>\n                <br />\n                <label htmlFor=\"author\">\n                    Autor\n                    <input\n                        id=\"author\"\n                        value={author}\n                        placeholder=\"Autor\"\n                        onChange={(event) => {\n                            setAuthor(event.target.value);\n                        }}\n                    />\n                </label>\n                <br />\n                <label htmlFor=\"publicationDate\">\n                    Data publikacji\n                    <input\n                        id=\"publicationDate\"\n                        value={publicationDate}\n                        placeholder=\"Publication Date\"\n                        onChange={(event) => {\n                            setPublicationDate(event.target.value);\n                        }}\n                    />\n                </label>\n                <br />\n                <GenreDropdown />\n                <br />\n                <AudienceDropdown />\n                <button onClick={(event) => addBook(event)}>Dodaj</button>\n            </form>\n        </div>\n    );\n};\n"},"lineCount":null}},"error":null,"hash":"7bb95f8e6ba68d4bac1f5dec959e3c8c","cacheData":{"env":{}}}